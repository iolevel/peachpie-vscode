{
    "name": "peachpie-vscode",
    "displayName": "Peachpie.VSCode",
    "description": "Integration of Peachpie - The PHP Compiler and Runtime for .NET",
    "version": "0.0.1",
    "publisher": "iolevel",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:peachpie.createProject"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "peachpie.createProject",
            "title": "Peachpie: Create Project"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}