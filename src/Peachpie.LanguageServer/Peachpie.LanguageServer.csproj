<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>0.9.11</VersionPrefix>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>Peachpie.LanguageServer</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>Peachpie.LanguageServer</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>StrongKeys\core.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Build.Runtime" Version="15.8.166" />
    <PackageReference Include="Microsoft.Build.NuGetSdkResolver" Version="4.8.1-*" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="Peachpie.CodeAnalysis" Version="0.9.15" />
    <PackageReference Include="Nuget.Common" Version="4.8.1-*" />
    <PackageReference Include="Nuget.Configuration" Version="4.8.1-*" />
    <PackageReference Include="Nuget.Frameworks" Version="4.8.1-*" />
    <PackageReference Include="Nuget.LibraryModel" Version="4.8.1-*" />
    <PackageReference Include="Nuget.Packaging" Version="4.8.1-*" />
    <PackageReference Include="Nuget.Packaging.Core" Version="4.8.1-*" />
    <PackageReference Include="Nuget.ProjectModel" Version="4.8.1-*" />
    <PackageReference Include="Nuget.Versioning" Version="4.8.1-*" />
  </ItemGroup>

</Project>
